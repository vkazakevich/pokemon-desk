{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "allowJs": true,
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "noLib": false,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "types": [
    "typePatches"
  ],
  "exclude": [
    "node_modules"
  ]
}
