{
  "name": "PokemonDesk",
  "version": "1.0.0",
  "scripts": {
    "build:dev": "env NODE_ENV=development webpack --config webpack.config.js",
    "build:prod": "env NODE_ENV=production webpack --config webpack.config.js",
    "start": "env NODE_ENV=development webpack-dev-server"
  },
  "main": "index.js",
  "repository": "https://github.com/vkazakevich/pokemon-desk.git",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.0",
    "classnames": "^2.2.6",
    "hookrouter": "^1.2.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.4.0",
    "@types/classnames": "^2.2.11",
    "@types/hookrouter": "^2.2.3",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "css-loader": "^5.0.0",
    "css-modules-typescript-loader": "^4.0.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "svg-react-loader": "^0.4.6",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.3",
    "typescript-plugin-css-modules": "^2.8.0",
    "url-loader": "^4.1.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "pretty-quick --staged",
      "eslint ./src"
    ],
    "src/**/*.{css,scss}": [
      "pretty-quick --staged"
    ]
  }
}
